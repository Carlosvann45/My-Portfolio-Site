{
    "version": 2,
    "builds": [
        {
            "src": "build/index.html",
            "use": "@vercel/static",
            "config": { "includeFiles": ["build/**"] }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "build/index.html"
        }
    ]
  }